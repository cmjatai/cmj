{
  "name": "v3",
  "version": "3.0.0",
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "ts-node ./server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr app/entry-server.ts ",
    "build:start": "NODE_ENV=production ts-node ./server",
    "prod": "npm run build && npm run build:start"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.0",
    "@types/node-fetch": "^2.5.12",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/server-renderer": "~3.2.0",
    "express": "^4.17.1",
    "ts-node": "~10.4.0",
    "typescript": "~4.5.0",
    "vite": "~2.6.0",
    "vue": "~3.2.0",
    "vue-router": "~4.0.0",
    "vuex": "~4.0.0"
  },
  "devDependencies": {
    "vite-plugin-pwa": "^0.11.5",
    "workbox-core": "^6.4.1",
    "workbox-precaching": "^6.4.1"
  }
}
